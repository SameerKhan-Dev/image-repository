<head>
	<title>Filtered Images By Search</title>
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css">
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js"></script>
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"></script>

</head>
<body>

    <header>
      <%- include('partials/header') %>
    </header>
    <div id="login">
      <h3 class="text-center text-danger pt-5">Results Matching Your Search:</h3>
      <div class="main-resource">
        <% for(let x = 0; x < allImages.length ; x++){ %>
          <article class="resource" style ="padding: 20px">
            <div class="info">
              <div>
                <a class = "resourceTitleTag" href=<%= allImages[x].url %>> <%= allImages[x].title%></a>
              </div>
              <span class="name"><b>Creator: </b><%= allImages[x].username %><span>
            </div>
            <p class="resource-data"><%= allImages[x].description%></p>
            <a>DELETE!</a>
          </article>
        <% } %>
      </div>
  </div>
</body>